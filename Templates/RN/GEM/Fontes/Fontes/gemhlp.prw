Static Function GMUpdSX3()

Local aArea := GetArea()

//
// deve atualizar os campos da tabela SUH para que o modulo SIGAFAT possa enxergar tb.
//
dbSelectArea("SX3")
SX3->(dbSetOrder(1))
If SX3->(dbSeek("SUH"))
	While SX3->(!Eof()) .AND. SX3->X3_ARQUIVO == "SUH"
		// se naum for o campo Filial
		If AllTrim(SX3->X3_CAMPO) != "UH_FILIAL" 
			If left(SX3->X3_USADO,1) <> CHR(132) // "ä"
				RecLock("SX3",.F.)
				SX3->X3_USADO := CHR(132)+Right(SX3->X3_USADO,14)
				MsUnlock()
			EndIf
		EndIf
		SX3->(dbSkip())
	EndDo
EndIf

RestArea(aArea)

Return( .T. )

