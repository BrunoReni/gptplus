//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
//³Caso o usuario seja o da Microsiga e a senha seja MICROS ( TESTE ), direciona ao ambiente de TESTES    ³
//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
If PadR( Upper( ::Question:PASSWORD ), 8 ) == PadR( "MICROS", 8 ) .And. StrZero( Val( ::Question:USERID ), 8 ) == "99194947" 
	cRespOri := HttpPost( URLSERASATEST, , "p=" + cString )   
Else
	cRespOri := HttpPost( URLSERASAPROD, , "p=" + cString )   
Endif

If ::Question:QUESTIONTYPE == "1" 

	//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
	//³ Layout relatorio                            ³
	//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
	lRetorno := .F. 
   
	If Len( cRespOri ) > 130 
	
		If SubStr( cRespOri, 131, 6 ) == "990000" 
		
			//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
			//³ Localiza o inicio o e final da consulta     ³
			//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
			nI := 261
			nJ := At( "FIM", cRespOri ) 
		
			cResposta := SubStr( cRespOri, nI, nJ - 1 )
			
			nTamReg := 130 
			
			nPos := 1 
			
			aRelato := {}    
			
			//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
			//³Obtem as linhas de resposta                  ³
			//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
			For nLoop := 1 To Int( Len( cResposta ) / nTamReg ) 
			
				cRegistro := SubStr( cResposta, nPos, nTamReg ) 
				
				nPos += nTamReg
				AAdd( aRelato, SubStr( cRegistro, 7, 79 ) ) 
			
			Next nLoop			
			
			lRetorno := .T. 
	
		EndIf			
		
	EndIf 	
	
Else

	//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
	//³ Layout Consulta                             ³
	//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
	//Inicio
	nI := At( INICIO, cRespOri )
	nJ := At( FIM, cRespOri )
	
	cResposta := Substr( cRespOri, nI, nJ - 1 )
	
	nI := At( INICIO, cResposta )
	nJ := At( BLOCO, cResposta )
	
	If nJ == 0
		nI := 0
		nJ := Len( cResposta )

		lRetorno := .F. 
		
	EndIf
	
	aAdd( aTemp, Substr( cResposta, nI, nJ - 1 ) )
	
EndIf 	

If lRetorno 	

	If ::Question:QUESTIONTYPE == "1"    
	
		//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
		//³ Layout relatorio                            ³
		//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
		::Record := WsClassNew( "SERASA_RecordView" )
		::Record:Header := {}

		AAdd( ::Record:Header, WsClassNew( "SERASA_HeaderView" ) )
		
		::Record:Header[Len( ::Record:Header )]:HeaderType := "Relatório"
		::Record:Header[Len( ::Record:Header )]:HeaderTypeDescription := "Relatório"
		::Record:Header[Len( ::Record:Header )]:Field := {}
		::Record:Header[Len( ::Record:Header )]:DataType := {}
		
		aAdd( ::Record:Header[Len( ::Record:Header )]:Field, "" )
		aAdd( ::Record:Header[Len( ::Record:Header )]:DataType, WsClassNew( "SERASA_DataTypeView" ) )
		
		::Record:Header[Len( ::Record:Header )]:DataType[Len( ::Record:Header[Len( ::Record:Header )]:DataType )]:Content := {}

		For nI := 1 To Len( aRelato )
			AAdd( ::Record:Header[Len( ::Record:Header )]:DataType[Len( ::Record:Header[Len( ::Record:Header )]:DataType )]:Content, aRelato[nI] )
		Next nI
	
	Else
	
		//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
		//³ Layout consulta                             ³
		//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ

		cResposta := Substr( cResposta, At( BLOCO, cResposta ) )
	
		//Bloco
		nTemp := 1
		
		While nTemp <> 0
			nI := At( BLOCO, cResposta )
			nJ := At( BLOCO, Substr( cResposta, nI + 1 ) )
			
			If nJ == 0
				nTemp := 0
				nJ := Len( cResposta )
			EndIf
			
			aAdd( aTemp, { Substr( cResposta, nI, nJ ) } )
			cResposta := Substr( cResposta, nJ + 1 )
		EndDo
		
		//Layout
		For nI := 2 To Len( aTemp )
			aAdd( aTemp[nI], {} )
			cBloco := aTemp[nI][1]
			nTemp := 1
			
			While nTemp <> 0
				nJ := At( LAYOUT, cBloco )
				nK := At( LAYOUT, Substr( cBloco, nJ + 1 ) )
				
				If nK == 0
					nTemp := 0
					nK := Len( cBloco )
				EndIf
				
				aAdd( aTemp[nI][2], {} )
				aAdd( aTemp[nI][2][ Len( aTemp[nI][2] ) ], Substr( cBloco, nJ + 2, 6 ) )
				aAdd( aTemp[nI][2][ Len( aTemp[nI][2] ) ], Substr( cBloco, nJ + 8, nK - 8 ) )
				cBloco := Substr( cBloco, nJ + nK )
			EndDo
		Next nI
		
		For nI := 2 To Len( aTemp )
			For nJ := 2 To Len( aTemp[nI] )
				For nK := 1 To Len( aTemp[nI][nJ] )
					nPos := 1
					aAdd( aTempLayout, {} )
					
					If Substr( aTemp[nI][nJ][nK][1], 5 ) == "99"
						nPosTipo := aScan( aTipos, { |x| x[1] == "XXYY99" } )
					Else
						nPosTipo := aScan( aTipos, { |x| x[1] == aTemp[nI][nJ][nK][1] } )
					EndIf
					
					If nPosTipo > 0
						aAdd( aTempLayout[Len( aTempLayout )], Substr( aTipos[nPosTipo][1], 1, 2 ) + " - " +;
																Substr( aTipos[nPosTipo][1], 3, 2 ) + " - " +;
																Substr( aTipos[nPosTipo][1], 5, 2 ) )
						aAdd( aTempLayout[Len( aTempLayout )], aTipos[nPosTipo][2][1] )
						aAdd( aTempLayout[Len( aTempLayout )], {} )
						
						For nL := 1 To Len( aTipos[nPosTipo][2][2] )
							aAdd( aTempLayout[Len( aTempLayout )][3], { aTipos[nPosTipo][2][2][nL][1], Substr( aTemp[nI][nJ][nK][2], nPos, aTipos[nPosTipo][2][2][nL][3] ) } )
	
							nPos += aTipos[nPosTipo][2][2][nL][3]
						Next nL
					Else
						varInfo( "*************** ERRO ***************", aTemp[nI][nJ][nK][1] )
					EndIf
				Next nK
			Next nJ
		Next nI
	
		::Record := WsClassNew( "SERASA_RecordView" )
		::Record:Header := {}
		
		For nI := 1 To Len( aTempLayout )
			nPosHeaderType := aScan( ::Record:Header, { |x| x:HeaderType == aTempLayout[nI][1] } )
			
			If nPosHeaderType > 0
				aAdd( ::Record:Header[nPosHeaderType]:DataType, WsClassNew( "SERASA_DataTypeView" ) )
				::Record:Header[nPosHeaderType]:DataType[Len( ::Record:Header[nPosHeaderType]:DataType )]:Content := {}
	
				If ValType( aTempLayout[nI][3] ) == "A"
					For nJ := 1 To Len( aTempLayout[nI][3] )
						aAdd( ::Record:Header[nPosHeaderType]:DataType[Len( ::Record:Header[nPosHeaderType]:DataType )]:Content, "" )
						
						::Record:Header[nPosHeaderType]:DataType[Len( ::Record:Header[nPosHeaderType]:DataType )]:Content[nJ] := aTempLayout[nI][3][nJ][2]
					Next nL
				EndIf
			Else
				aAdd( ::Record:Header, WsClassNew( "SERASA_HeaderView" ) )
	
				::Record:Header[Len( ::Record:Header )]:HeaderType := aTempLayout[nI][1]
				::Record:Header[Len( ::Record:Header )]:HeaderTypeDescription := aTempLayout[nI][2]
				::Record:Header[Len( ::Record:Header )]:Field := {}
				::Record:Header[Len( ::Record:Header )]:DataType := {}
	
				aAdd( ::Record:Header[Len( ::Record:Header )]:DataType, WsClassnew( "SERASA_DataTypeView" ) )
				::Record:Header[Len( ::Record:Header )]:DataType[Len( ::Record:Header[Len( ::Record:Header )]:DataType )]:Content := {}
	
				If ValType( aTempLayout[nI][3] ) == "A"
					For nJ := 1 To Len( aTempLayout[nI][3] )
						aAdd( ::Record:Header[Len( ::Record:Header )]:Field, "" )
						aAdd( ::Record:Header[Len( ::Record:Header )]:DataType[Len( ::Record:Header[Len( ::Record:Header )]:DataType )]:Content, "" )
					
						::Record:Header[Len( ::Record:Header )]:Field[nJ] := aTempLayout[nI][3][nJ][1]
						::Record:Header[Len( ::Record:Header )]:DataType[Len( ::Record:Header[Len( ::Record:Header )]:DataType )]:Content[nJ] := aTempLayout[nI][3][nJ][2]
					Next nL
				Else
					SetSoapFault( "ERRO", aResultado[nI][11][2] )
					lRetorno := .F.
				EndIf
			EndIf
		Next nI
		
	EndIf  
	
Else
	
	SetSoapFault( "ERRO", Left( cRespOri, Len( cRespOri ) - 2 ) )
		
EndIf 	
		
Return lRetorno

