	If lLogTissOn
		PlsPtuLog(PLSRetTime() + " - Finalizando tissVerificaElegibilidade (WSPLSTISSO)", "tissonanalise.log")
	EndIf

Return StrTran( StrTran( StrTran( cRet ,Chr(13),""),Chr(10),""),Chr(9),"")
