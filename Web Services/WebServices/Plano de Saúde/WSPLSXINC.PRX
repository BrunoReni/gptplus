		BE2->( DbSetOrder(1) )
		If BE2->( MsSeek( xFilial("BE2")+::NumAut ) )
			If BD5->(MsSeek(xFilial("BD5")+BE2->(BE2_OPEMOV+BE2_CODLDP+BE2_CODPEG+BE2_NUMERO)))
				BD5->(Reclock("BD5",.F.))

				BD5->BD5_GESTAN:= "1"

				BD5->(MsUnlock())
			Else
				::WSNULL := "false|Guia não encontrada"
			Endif
		Else
			::WSNULL := "false|Autorizacao nao encontrada"
		Endif

