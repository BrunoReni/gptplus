/*/
WSMETHOD GetDailyNews WSRECEIVE NULLPARAM WSSEND DailyNews WSSERVICE UserPresentation

Local cFile := SuperGetMv("MV_PORTAL3",.F.)
Local aHtml := {}
Local cHtml := ""
Local nX    := 0    
  
If File(cFile)
	FT_FUse(cFile)
	FT_FGotop()
	While ( !FT_FEof() )
		cHtml := FT_FREADLN()
		nX := At("=",cHtml)
		aadd(aHtml,{SubStr(cHtml,1,nX-1),SubStr(cHtml,nX+1)})
		FT_FSkip()
	EndDo
	FT_FUse()
Else
	aHtml := {{STR0007,STR0008}} //"Configuração"###"Preencha o paramêtro MV_PORTAL3 com um arquivo que contenha um texto normal ou com uso de tag´s html. Utilize o sinal de igual para identificar o cabeçalho da noticia. Exemplo: Promoções=10% de desconto em todos os produtos na linha de Software"
EndIf 

::DailyNews := {}
For nX := 1 To Len(aHtml)
	aadd(::DailyNews,WsClassNew("GENERICSTRUCT"))
	::DailyNews[nX]:Code := aHtml[nX][1]
	::DailyNews[nX]:Description := aHtml[nX][2]
Next nX
Return(.T.)

