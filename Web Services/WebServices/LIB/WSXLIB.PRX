Function WsChangeModule( cModName )

Local lRetorno 	:= .T.
Local nX 		:= 0

Static aModulos := RetModName()

nX := aScan( aModulos,{ |x| AllTrim( x[2] ) == cModName } )

If nX <> 0
	nModulo := aModulos[nX][1]
	cModulo := AllTrim( SubStr( aModulos[nX][2], 5 ) )
Else
	nModulo := 0
	cModulo := "CFG"
EndIf

Return lRetorno


