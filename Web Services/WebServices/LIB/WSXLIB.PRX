/*/
Function MsAuto2GDb(aHeader,cAlias)

Local aArea  := GetArea()
Local aItem  := {}
Local aItens := {}
Local nY     := 0

dbSelectArea(cAlias)
dbGotop()
While !Eof()
	aItem := {}
	For nY := 1 To Len(aHeader)
		aadd(aItem,{aHeader[nY][2],FieldGet(FieldPos(aHeader[nY][2])),Nil})
	Next nY
	aadd(aItens,aItem)
	dbSelectArea(cAlias)	
	dbSkip()
EndDo

RestArea(aArea)
Return(aItens)

