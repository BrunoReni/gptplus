Function MsAuto2Gd(aHeader,aCols)

Local aItem  := {}
Local aItens := {}
Local nX     := 0
Local nY     := 0

For nX := 1 To Len(aCols)
	aItem := {}
	If !aCols[nX][Len(aHeader)+1]
		For nY := 1 To Len(aHeader)
			aadd(aItem,{aHeader[nY][2],aCols[nX][nY],Nil})
		Next nY
		aadd(aItens,aItem)
	EndIf
Next nX

Return(aItens)

