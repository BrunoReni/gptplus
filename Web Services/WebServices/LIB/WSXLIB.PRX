	If ((aAuto[nX][2] <> Nil .And. !lEmpty) .Or. (lEmpty .And. !Empty(aAuto[nX][2]) )) .And. Eval(bAdd)
		aadd(aReturn,aAuto[nX])	
	EndIf
Next nX

Return(aReturn)

