*/                                     
Method ValOK() Class LjCCadProTabInt

	Local aArea		:= GetArea()				//Guarda a Area
	Local cProcesso	:= M->MDP_PROCESS			//Codigo do Processo
	Local cTabela	:= M->MDP_TABELA			//Alias da Tabela
	Local lRetorno	:= .T.						//Retorno da Funcao
	
	If INCLUI
		DbSelectArea("MDP")
		DbSetOrder(1)	//Filial + Processo + Tabela
		
		If DbSeek(xFilial("MDP") + cProcesso + cTabela)
			lRetorno := .F.
			MsgAlert(STR0002)		//"Ja existe um Registro com este Codigo."
		EndIf
	EndIf
	
	RestArea(aArea)

Return lRetorno
