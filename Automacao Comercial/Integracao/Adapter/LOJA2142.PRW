Method ValOK() Class LjCCadParamInt

	Local aArea		:= GetArea()				//Guarda a Area
	Local cCodigo	:= M->MDQ_CODIGO			//Codigo do parametro
	Local lRetorno	:= .T.						//Retorno da Funcao
	
	If INCLUI
		DbSelectArea("MDQ")
		DbSetOrder(1)	//Filial + codigo
		
		If DbSeek(xFilial("MDQ") + cCodigo)
			lRetorno := .F.
			MsgAlert(STR0002)		//"Já existe um registro com este codigo."
		ElseIf cCodigo <> "001"
			lRetorno := .F.
			MsgAlert(STR0003)		//"Não é possível efetuar o cadastro com o codigo diferente 001."		
		EndIf
	ElseIf ALTERA
		If cCodigo <> "001"
			lRetorno := .F.
			MsgAlert(STR0004)		//"O Código não pode ser alterado."		
		EndIf	
	EndIf
	
	RestArea(aArea)

Return lRetorno